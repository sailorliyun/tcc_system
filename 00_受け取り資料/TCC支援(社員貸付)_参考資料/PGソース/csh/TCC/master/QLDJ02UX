###################################################################
# STEP-3.23     ストアド・プロシジャの実行      総振台帳（事務部）#
###################################################################
set    KINOU      = 'クレジット帳票：現行日次処理(総振台帳（事務部）)'

$LOGWRITE "$MODULE" I "開始:$KINOU" "$USERKEY"  # 開始メッセージの出力

setenv NLS_LANG Japanese_Japan.JA16SJIS
set    PRM_Data = "'23'"                         #付与ﾊﾟﾗﾒｰﾀ

$EXE_DIR/qusc_sspexec.exe  $JOBNET_ID  $JOB_ID  "QVAP_S000200Pkg.QVAP_S000200( $PRM_JOBNET, $PRM_JOB,$PRM_Data)"

set rc = $status
setenv NLS_LANG Japanese_Japan.JA16EUC

if ( $rc != 0 ) then
        $LOGWRITE  "$MODULE"  E  "失敗:$KINOU  RC=[$rc]"  "$USERKEY"
        exit  1                                  # 異常終了
endif

$LOGWRITE "$MODULE" I "終了:$KINOU" "$USERKEY"  # 終了メッセージの出力


#DEL 1006060 ｸﾚｼﾞｯﾄ再構築帳票削減           20110721 FIP.Eriguchi Start
