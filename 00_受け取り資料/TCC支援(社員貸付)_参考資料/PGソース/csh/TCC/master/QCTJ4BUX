       where QTAU_PRMTABLEMEI = '$TABLE_NAME'
         and QTAU_PRMPARTITIONMEI = '$OLD_PART';
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
