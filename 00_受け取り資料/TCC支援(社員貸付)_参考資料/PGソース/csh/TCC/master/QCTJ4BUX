#!/bin/csh
#   スクリプト名 ： QCTJ4BUX
#   機  能       ： CS利用明細行ファイルのパーティションシフトを行う
#   作成者       ： FIP
#
#   修正履歴
#   項番     修正日      修正者         修正内容
#   00001    2007/03/12  nbc.hayashi    MAXエクステント数変更
#   000002   2009/03/29  NBC.ISHIDA     非互換対応
#            2009/03/29  NBC.ISHIDA     非互換対応
#            2009/03/29  NBC.ISHIDA     INITIAL値・NEXT値のサイズ変更

##################################
#           環境設定             #
##################################
set    SHELL_NAME = $0                                 # シェル名
setenv JOB_ID       $SHELL_NAME:t                      # ジョブ名
setenv JOBNET_ID   'QCT261UX'                          # ジョブネット名
set    LOGFILE    = $USER_LOG_DIR/${JOB_ID}.log        # ログファイル名
set    LOGWRITE   = $EXE_DIR/QUAJ_MSGLOG.BAT           # ログ出力モジュール
setenv USERKEY      'QCTJ4BUX'                         # r%qzrtrkqz
set    KINOU      = 'CS利用明細行ファイル登録'

set TABLE_NAME = QCS4_RIYOUDTLGT            # 対象テーブル名
set STEP_F   = $DATA4_DIR/${JOB_ID}_step.tmp
set BKUP_DIR = $QC_DATA_DIR/qcs/data/shift_work

set MODULE = 'SHELL'
$LOGWRITE "$MODULE" I "開始:$KINOU" "$USERKEY"   # 開始メッセージの出力

set year  = `date +%Y`                     # 処理年 (システム日付より)
set month = `date +%m`                     # 処理月 (システム日付より)

# 処理月Leading Zero Suppress
@ month ++
@ month --

if ( $month < 10 ) then
        set month = 0$month
endif

#翌月取得
set year2  = $year
set month2 = $month
@ month2 ++
if ( $month2 == 13 ) then
        @ year2 ++
        set month2 = 1
endif
if ( $month2 < 10 ) then
        set month2 = 0$month2
endif
set  newym = $year2$month2
set  newdd = 01

##################################
# STEP-1 当月パーティション検索  #
##################################
set MODULE = 'SQL*PLUS-1'
$LOGWRITE "$MODULE" I "開始:当月パーティション検索" $USERKEY
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
set sqlp ""
set sqln off
set pause off
set hea   off
set lin 200
column QTAU_PRMPARTITIONMEI format A50
column QTAU_TABLESPACEMEI format A50
select max( QTAU_BOUNDTI )
       from QTAU_PARTTABLET
       where QTAU_PRMTABLEMEI = '$TABLE_NAME'
           and QTAU_BOUNDTI != ( select max( QTAU_BOUNDTI )
                                   from QTAU_PARTTABLET
                                   where QTAU_PRMTABLEMEI = '$TABLE_NAME');
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:当月パーティション検索 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

set val = `grep "^[0-9]" $LOGFILE`
if ( "$val" == "" ) then
        set MSG = "失敗:当月パーティション検索（該当無し） "
        $LOGWRITE  "$MODULE"  E  "$MSG" "$USERKEY"
        exit  1
endif

set ZEN_BOUND = $val[1]

if ( $ZEN_BOUND != $year$month ) then
        set MSG = "失敗:処理年月が間違っています[$ZEN_BOUND]"
        $LOGWRITE  "$MODULE"  E  "$MSG" "$USERKEY"
        exit  1
endif

$LOGWRITE "$MODULE" I "終了:当月パーティション検索" $USERKEY

##################################
# STEP-2 最新パーティション検索  #
##################################
set MODULE = 'SQL*PLUS-2'
$LOGWRITE "$MODULE" I "開始:最新パーティション検索" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
set sqlp ""
set sqln off
set pause off
set hea   off
set lin 200
column QTAU_PRMPARTITIONMEI format A50
column QTAU_TABLESPACEMEI format A50
select QTAU_PRMPARTITIONMEI,QTAU_TABLESPACEMEI
       from QTAU_PARTTABLET
       where QTAU_PRMTABLEMEI = '$TABLE_NAME'
           and QTAU_BOUNDTI = ( select max( QTAU_BOUNDTI )
                                  from QTAU_PARTTABLET
                                  where QTAU_PRMTABLEMEI = '$TABLE_NAME');
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:最新パーティション検索 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

set val = `grep QCS4 $LOGFILE`
if ( "$val" == "" ) then
        set MSG = "失敗:最新パーティション検索（該当無し） "
        $LOGWRITE  "$MODULE"  E  "$MSG" "$USERKEY"
        exit  1
endif

set MAX_PART  = $val[1]
set MAX_SPACE = $val[2]

$LOGWRITE "$MODULE" I "終了:最新パーティション検索" $USERKEY

##################################
# STEP-3 最古パーティション検索  #
##################################
set MODULE = 'SQL*PLUS-3'
$LOGWRITE "$MODULE" I "開始:最古パーティション検索" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
set sqlp ""
set sqln off
set pause off
set hea   off
set lin 200
column QTAU_PRMPARTITIONMEI format A50
column QTAU_TABLESPACEMEI format A50
select QTAU_PRMPARTITIONMEI,QTAU_TABLESPACEMEI
       from QTAU_PARTTABLET
       where QTAU_PRMTABLEMEI = '$TABLE_NAME'
           and QTAU_BOUNDTI = ( select min( QTAU_BOUNDTI )
                                 from QTAU_PARTTABLET
                                 where QTAU_PRMTABLEMEI = '$TABLE_NAME');
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:最古パーティション検索 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

set val = `grep QCS4 $LOGFILE`
if ( "$val" == "" ) then
        set MSG = "失敗:最古パーティション検索（該当無し） "
        $LOGWRITE  "$MODULE"  E  "$MSG" "$USERKEY"
        exit  1
endif

set OLD_PART  = $val[1]
set OLD_SPACE = $val[2]

$LOGWRITE "$MODULE" I "終了:最古パーティション検索" $USERKEY

##################################
# STEP-4 主キー制約削除          #
##################################
STEP_PRM_KEY_DROP_PROC:

set MODULE = 'SQL*PLUS-4'
$LOGWRITE "$MODULE" I "開始:主キー制約削除" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
alter table $TABLE_NAME drop primary key;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:主キー制約削除 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

#主キーの表領域をまとめる
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S tccadmin/tccadmin << EOF > $LOGFILE
sqlplus -S $DB_ADMINID/$DB_ADMINPASS@$ORACLE_SID << EOF > $LOGFILE
alter tablespace CSRIYOUIDX coalesce;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:主キー制約削除（圧縮） RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
endif

$LOGWRITE "$MODULE" I "終了:主キー制約削除" $USERKEY

##################################
# STEP-5 最古パーティション削除  #
##################################
STEP_OLD_PART_DROP_PROC:

set MODULE = 'SQL*PLUS-5'
$LOGWRITE "$MODULE" I "開始:最古パーティション削除" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
alter table $TABLE_NAME drop partition $OLD_PART;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:最古パーティション削除 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

#最古パーティションの表領域をまとめる
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S tccadmin/tccadmin << EOF > $LOGFILE
sqlplus -S $DB_ADMINID/$DB_ADMINPASS@$ORACLE_SID << EOF > $LOGFILE
alter tablespace $OLD_SPACE coalesce;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:最古パーティション削除（最古圧縮） RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
endif

$LOGWRITE "$MODULE" I "終了:最古パーティション削除" $USERKEY

##################################
# STEP-6 最新パーティション削除  #
##################################
STEP_MAX_PART_DROP_PROC:

set MODULE = 'SQL*PLUS-6'
$LOGWRITE "$MODULE" I "開始:最新パーティション削除" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
alter table $TABLE_NAME drop partition $MAX_PART;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:最新パーティション削除 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

#最新パーティションの表領域をまとめる
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S tccadmin/tccadmin << EOF > $LOGFILE
sqlplus -S $DB_ADMINID/$DB_ADMINPASS@$ORACLE_SID << EOF > $LOGFILE
alter tablespace $MAX_SPACE coalesce;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:最新パーティション削除（最新圧縮） RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
endif

$LOGWRITE "$MODULE" I "終了:最新パーティション削除" $USERKEY

##################################
# STEP-7 当月パーティション追加  #
##################################
STEP_GEN_PART_ADD_PROC:

set MODULE = 'SQL*PLUS-7'
$LOGWRITE "$MODULE" I "開始:当月パーティション追加" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
alter table $TABLE_NAME add partition $OLD_PART
            values less than (to_date('$newym$newdd','YYYYMMDD'))
            tablespace $OLD_SPACE;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:当月パーティション追加 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

$LOGWRITE "$MODULE" I "終了:当月パーティション追加" $USERKEY

##################################
# STEP-8 最新パーティション追加  #
##################################
STEP_MAX_PART_ADD_PROC:

set MODULE = 'SQL*PLUS-8'
$LOGWRITE "$MODULE" I "開始:最新パーティション追加" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
alter table $TABLE_NAME add partition $MAX_PART
       values less than (MAXVALUE)
       tablespace $MAX_SPACE;
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:最新パーティション追加 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

$LOGWRITE "$MODULE" I "終了:最新パーティション追加" $USERKEY

##################################
# STEP-9 データロード            #
##################################
STEP_SQLLDR_PROC:

set MODULE = 'SQL*LOADER'
$LOGWRITE "$MODULE" I "開始:データロード" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlldr $DB_USERID/$DB_PASSWORD                         \
sqlldr $DB_USERID/$DB_PASSWORD@$ORACLE_SID                         \
   control=$LDR_CTRL_DIR/qcs4_riyoudtlgt.sql.shiftbackup   \
   data=$BKUP_DIR/qcsf_riyougyou.dat.shiftbackup.1       \
   log=$LDR_LOG_DIR/qcs4_riyoudtlgt.log.shiftbackup.1        \
   direct=true \
             > $LOGFILE
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(1) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

#----------------------------------------------------------#
#   インデックスエラーのチェック
#----------------------------------------------------------#
rm -f ${LOGFILE}2 ${LOGFILE}3
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > ${LOGFILE}2
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > ${LOGFILE}2
set lin 200
select index_name,status from user_indexes
       where table_name = '$TABLE_NAME'  and
             status     = 'UNUSABLE';
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(1)(sqlplus) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
grep "UNUSABLE" `find ${USER_LOG_DIR} -name "${JOB_ID}.log2" -print` > ${LOGFILE}3
if !(-z ${LOGFILE}3 ) then
        set MSG = "失敗:データロード(1)(grep) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
#----------------------------------------------------------#

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlldr $DB_USERID/$DB_PASSWORD                         \
sqlldr $DB_USERID/$DB_PASSWORD@$ORACLE_SID                         \
   control=$LDR_CTRL_DIR/qcs4_riyoudtlgt.sql.shiftbackup   \
   data=$BKUP_DIR/qcsf_riyougyou.dat.shiftbackup.2       \
   log=$LDR_LOG_DIR/qcs4_riyoudtlgt.log.shiftbackup.2        \
   direct=true \
             > $LOGFILE
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(2) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

#----------------------------------------------------------#
#   インデックスエラーのチェック
#----------------------------------------------------------#
rm -f ${LOGFILE}2 ${LOGFILE}3

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > ${LOGFILE}2
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > ${LOGFILE}2
set lin 200
select index_name,status from user_indexes
       where table_name = '$TABLE_NAME'  and
             status     = 'UNUSABLE';
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(2)(sqlplus) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
grep "UNUSABLE" `find ${USER_LOG_DIR} -name "${JOB_ID}.log2" -print` > ${LOGFILE}3
if !(-z ${LOGFILE}3 ) then
        set MSG = "失敗:データロード(2)(grep) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
#----------------------------------------------------------#
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlldr $DB_USERID/$DB_PASSWORD                         \
sqlldr $DB_USERID/$DB_PASSWORD@$ORACLE_SID                         \
   control=$LDR_CTRL_DIR/qcs4_riyoudtlgt.sql.shiftbackup   \
   data=$BKUP_DIR/qcsf_riyougyou.dat.shiftbackup.3       \
   log=$LDR_LOG_DIR/qcs4_riyoudtlgt.log.shiftbackup.3        \
   direct=true \
             > $LOGFILE
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(3) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

#----------------------------------------------------------#
#   インデックスエラーのチェック
#----------------------------------------------------------#
rm -f ${LOGFILE}2 ${LOGFILE}3

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > ${LOGFILE}2
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > ${LOGFILE}2
set lin 200
select index_name,status from user_indexes
       where table_name = '$TABLE_NAME'  and
             status     = 'UNUSABLE';
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(3)(sqlplus) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
grep "UNUSABLE" `find ${USER_LOG_DIR} -name "${JOB_ID}.log2" -print` > ${LOGFILE}3
if !(-z ${LOGFILE}3 ) then
        set MSG = "失敗:データロード(3)(grep) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
#----------------------------------------------------------#
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlldr $DB_USERID/$DB_PASSWORD                         \
sqlldr $DB_USERID/$DB_PASSWORD@$ORACLE_SID                         \
   control=$LDR_CTRL_DIR/qcs4_riyoudtlgt.sql.shiftbackup   \
   data=$BKUP_DIR/qcsf_riyougyou.dat.shiftbackup.4       \
   log=$LDR_LOG_DIR/qcs4_riyoudtlgt.log.shiftbackup.4        \
   direct=true \
             > $LOGFILE
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(4) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

#----------------------------------------------------------#
#   インデックスエラーのチェック
#----------------------------------------------------------#
rm -f ${LOGFILE}2 ${LOGFILE}3
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > ${LOGFILE}2
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > ${LOGFILE}2
set lin 200
select index_name,status from user_indexes
       where table_name = '$TABLE_NAME'  and
             status     = 'UNUSABLE';
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:データロード(4)(sqlplus) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
grep "UNUSABLE" `find ${USER_LOG_DIR} -name "${JOB_ID}.log2" -print` > ${LOGFILE}3
if !(-z ${LOGFILE}3 ) then
        set MSG = "失敗:データロード(4)(grep) RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif
#----------------------------------------------------------#

$LOGWRITE "$MODULE" I "終了:データロード" $USERKEY

##################################
# STEP-10 主キー制約付与         #
##################################
STEP_PRM_KEY_ADD_PROC:

set MODULE = 'SQL*PLUS-10'
$LOGWRITE "$MODULE" I "開始:主キー制約付与" $USERKEY

#00001  start  MAXエクステント修正 MAXEXTENTS:100 → MAXEXTENTS:180
##20050314 インデックス領域サイズ修正 I:2000M N:200M → I:200M N:20M
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
#alter table $TABLE_NAME add
#       PRIMARY KEY (QCS4_PRMKigyouCd, QCS4_PRMSubRangeKey,
#              QCS4_PRMNaibuKaiinNo, QCS4_PRMTorihikiNo,
#              QCS4_PRMGyouNo)
#       USING INDEX
#              TABLESPACE CSRIYOUIDX
#              STORAGE (
#                     INITIAL    200M
#                     NEXT        20M
#                     MINEXTENTS    1
#                     MAXEXTENTS  100
#                     PCTINCREASE   0
#              )
#       NOLOGGING
#;
#quit SQL.SQLCODE
#EOF

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > $LOGFILE
#alter table $TABLE_NAME add
#       PRIMARY KEY (QCS4_PRMKigyouCd, QCS4_PRMSubRangeKey,
#              QCS4_PRMNaibuKaiinNo, QCS4_PRMTorihikiNo,
#              QCS4_PRMGyouNo)
#       USING INDEX
#              TABLESPACE CSRIYOUIDX
#              STORAGE (
#                     INITIAL    200M
#                     NEXT        20M
#                     MINEXTENTS    1
#                     MAXEXTENTS  180
#                     PCTINCREASE   0
#              )
#       NOLOGGING
#;
#quit SQL.SQLCODE
#EOF

sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
alter table $TABLE_NAME add
       PRIMARY KEY (QCS4_PRMKigyouCd, QCS4_PRMSubRangeKey,
              QCS4_PRMNaibuKaiinNo, QCS4_PRMTorihikiNo,
              QCS4_PRMGyouNo)
       USING INDEX
              TABLESPACE CSRIYOUIDX
              STORAGE (
                     INITIAL     51M
                     NEXT         5M
                     MINEXTENTS    1
                     MAXEXTENTS  180
                     PCTINCREASE   0
              )
       NOLOGGING
;
quit SQL.SQLCODE
EOF

#00001  end  MAXエクステント修正 MAXEXTENTS:100 → MAXEXTENTS:180
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:主キー制約付与 RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
        exit  1
endif

$LOGWRITE "$MODULE" I "終了:主キー制約付与" $USERKEY

