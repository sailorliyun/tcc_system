#   log=$LDR_LOG_DIR/qted_seikyukai01.log          \
#   direct=true                                    \
#               > $LOGFILE
sqlldr $DB_USERID/$DB_PASSWORD@$ORACLE_SID         \
   control=$LDR_CTRL_DIR/qhed_seikyukai.sql       \
   data=$SEIKYU_DATA_DIR/qhef_seikyukai01.dat \
   log=$LDR_LOG_DIR/qted_seikyukai01.log          \
   direct=true                                    \
               > $LOGFILE
#MOD 0807073 クレジット支援システムの再構築 20090329 shibuichi end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 shibuichi end

set rc = $status
if ( $rc != 0 ) then
	set MSG = "失敗:QHE3 データロード１ RC=[$rc]"
	$LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
	exit  1
endif

#echo "STEP_2" >> $STEP_F

#000001 ADD NBC.HAYASHI 2006/11/15 START
#----------------------------------------------------------#
#   インデックスエラーのチェック
#----------------------------------------------------------#
