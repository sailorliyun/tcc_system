sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
alter table $TABLE_NAME add
       PRIMARY KEY (QHE3_KeyRecShikibetsu, QHE3_KeySeikyuYm,
                    QHE3_KeySeqNo,         QHE3_KeyBunkatsuNo,
                    QHE3_KeyVersionNo,     QHE3_KeyRecNo)
       USING INDEX  NOLOGGING
              TABLESPACE SEI1IDX02
              STORAGE (
                     INITIAL 100M
                     NEXT 10M
                     MINEXTENTS 1
                     MAXEXTENTS 100
                     PCTINCREASE 0
              );
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 shibuichi end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 shibuichi end

set rc = $status
if ( $rc != 0 ) then
	set MSG = "失敗:QHE3 主キー制約付与 RC=[$rc]"
	$LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
	exit  1
endif

#echo "STEP_10" >> $STEP_F

$LOGWRITE "$MODULE" I "終了:QHE3 主キー制約付与" $USERKEY

##########################################
# STEP-99      後始末                    #
##########################################
STEP_99:

#/bin/rm $STEP_F

#rm -f $QH_DATA_DIR/monthly/qhaf_seikyukai??.dat

set MODULE = 'SHELL'
$LOGWRITE "$MODULE" I "終了:$KINOU" "$USERKEY"   # 終了メッセージの出力

exit 0
