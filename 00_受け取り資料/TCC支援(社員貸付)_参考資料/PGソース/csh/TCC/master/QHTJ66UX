#ADD 0807073 クレジット支援システムの再構築 20090427 NBC.ISHIDA start
#ADD 0808052 ＴＣＣ支援システムの再構築     20090427 NBC.ISHIDA start
##################################
# STEP-1.1 インデックス削除        #
##################################
set MODULE = 'SQL*PLUS-1'
$LOGWRITE "$MODULE" I "開始:インデックス削除" $USERKEY

sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
drop index XIEQHE8_SEIITIRANDT;
quit SQL.SQLCODE
EOF

set rc = $status
if ( $rc != 0 ) then
	set MSG = "失敗:インデックス削除 RC=[$rc]"
	$LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
	exit  1
endif

#表領域をまとめる
sqlplus -S $DB_ADMINID/$DB_ADMINPASS@$ORACLE_SID << EOF > $LOGFILE
alter tablespace SEI1IDX01 coalesce;
quit SQL.SQLCODE
EOF

set rc = $status
if ( $rc != 0 ) then
        set MSG = "失敗:インデックス削除（圧縮） RC=[$rc]"
        $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
endif

$LOGWRITE "$MODULE" I "終了:インデックス削除" $USERKEY

#ADD 0807073 クレジット支援システムの再構築 20090427 NBC.ISHIDA end
#ADD 0808052 ＴＣＣ支援システムの再構築     20090427 NBC.ISHIDA end

##################################
# STEP-2 請求一覧データロード１  #
##################################
STEP_SQLLDR_PROC_1:

#set cnt = `grep -c "STEP2" $STEP_F`
#if ( $cnt > 0 ) then
##	goto STEP_PRM_KEY_ADD_PROC
#	goto STEP_END_PROC
#endif

set MODULE = 'SQL*LOADER'
set STEPKINOU = 'QHE8 請求一覧データロード１'
$LOGWRITE "$MODULE" I "開始:$STEPKINOU" $USERKEY

#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlldr $DB_USERID/$DB_PASSWORD                          \
sqlldr $DB_USERID/$DB_PASSWORD@$ORACLE_SID                          \
   control=$LDR_CTRL_DIR/qhed_seiitiran_shift.sql       \
   data=$QH_SHIFT_DIR/qhef_qhe8itiran01.dat.shiftbackup \
   log=$LDR_LOG_DIR/qhed_seiitiran_shift01.log          \
   direct=true                                          \
               > $LOGFILE
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
	set MSG = "失敗:$STEPKINOU RC=[$rc]"
	$LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
	exit  1
endif

#----------------------------------------------------------#
#   インデックスエラーのチェック              2000/08/15
#----------------------------------------------------------#
rm -f ${LOGFILE}2 ${LOGFILE}3
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA start
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA start
#sqlplus -S $DB_USERID/$DB_PASSWORD << EOF > ${LOGFILE}2
sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > ${LOGFILE}2
set lin 200
select index_name,status from user_indexes
       where table_name = '$TABLE_NAME'  and
             status     = 'UNUSABLE';
quit SQL.SQLCODE
EOF
#MOD 0807073 クレジット支援システムの再構築 20090329 NBC.ISHIDA end
#MOD 0808052 ＴＣＣ支援システムの再構築     20090329 NBC.ISHIDA end

set rc = $status
if ( $rc != 0 ) then
	set MSG = "失敗:$STEPKINOU（sqlplus） RC=[$rc]"
	$LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
	exit  1
endif
grep "UNUSABLE" `find ${USER_LOG_DIR} -name "${JOB_ID}.log2" -print` > ${LOGFILE}3
if !(-z ${LOGFILE}3 ) then
	set MSG = "失敗:$STEPKINOU（grep） RC=[$rc]"
	$LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
	exit  1
endif

#echo "STEP2" >> $STEP_F

$LOGWRITE "$MODULE" I "終了:$STEPKINOU" $USERKEY

##################################
# STEP-3  主キー制約付与         #
##################################
#STEP_PRM_KEY_ADD_PROC:
