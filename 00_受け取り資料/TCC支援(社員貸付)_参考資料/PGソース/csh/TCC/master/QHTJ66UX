#compress -f $QH_SHIFT_DIR/qhef_qhe8itiran??.dat.shiftbackup
#
##echo "STEP3" >> $STEP_F
#
#$LOGWRITE "$MODULE" I "終了:$STEPKINOU" $USERKEY
#
#ADD 0807073 クレジット支援システムの再構築 20090427 NBC.ISHIDA start
#ADD 0808052 ＴＣＣ支援システムの再構築     20090427 NBC.ISHIDA start
##################################
# STEP-3.1  インデックス作成     #
##################################
set MODULE = 'SQL*PLUS-2'
$LOGWRITE "$MODULE" I "開始:インデックス作成" $USERKEY

sqlplus -S $DB_USERID/$DB_PASSWORD@$ORACLE_SID << EOF > $LOGFILE
CREATE INDEX XIEQHE8_SEIITIRANDT ON QHE8_SEIITIRANDT
(QHE8_KIGYOCD, QHE8_SUBRNG, QHE8_NAIBUKAIIN)
PCTFREE 10
INITRANS 2
MAXTRANS 255
TABLESPACE SEI1IDX01
STORAGE(
        INITIAL 210M
        NEXT 21M
        MINEXTENTS 1
        MAXEXTENTS 100
        PCTINCREASE 0
        FREELISTS 1
        FREELIST GROUPS 1
        BUFFER_POOL DEFAULT
       );
quit SQL.SQLCODE
EOF

set rc = $status
if ( $rc != 0 ) then
    set MSG = "失敗:インデックス作成 RC=[$rc]"
    $LOGWRITE  "$MODULE"  E  "$MSG" $USERKEY
    exit  1
endif

$LOGWRITE "$MODULE" I "終了:インデックス作成" $USERKEY

#ADD 0807073 クレジット支援システムの再構築 20090427 NBC.ISHIDA end
#ADD 0808052 ＴＣＣ支援システムの再構築     20090427 NBC.ISHIDA end
##########################################
# STEP-99      後始末                    #
##########################################
STEP_END_PROC:

#/bin/rm $STEP_F

#compress -f $QH_SHIFT_DIR/qhef_qhe8itiran??.dat.shiftbackup

set MODULE = 'SHELL'
$LOGWRITE "$MODULE" I "終了:$KINOU" "$USERKEY"   # 終了メッセージの出力

exit 0
