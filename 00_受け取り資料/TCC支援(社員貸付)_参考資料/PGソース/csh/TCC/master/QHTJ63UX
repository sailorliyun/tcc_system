       PRIMARY KEY (QHE3_KeyRecShikibetsu, QHE3_KeySeikyuYm,
              QHE3_KeySeqNo, QHE3_KeyBunkatsuNo, QHE3_KeyVersionNo,
              QHE3_KeyRecNo)
