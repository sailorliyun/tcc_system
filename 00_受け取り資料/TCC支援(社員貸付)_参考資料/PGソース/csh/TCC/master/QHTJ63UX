STEP_MAX_PART_DROP_PROC:

#set cnt = `grep -c "STEP6" $STEP_F`
#if ( $cnt > 0 ) then
#	goto STEP_GEN_PART_ADD_PROC
#endif

set MODULE = 'SQL*PLUS-6'
set STEPKINOU = 'QHE3 最新パーティション削除'
