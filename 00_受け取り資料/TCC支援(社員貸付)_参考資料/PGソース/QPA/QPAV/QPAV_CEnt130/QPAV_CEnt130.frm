    mdblKyuZanSum = 0           'èÿèëÅEããó^écçÇåv
    mdblSyoZanSum = 0           'èÿèëÅEè‹ó^écçÇåv

    'ëçåèêî
    lngKensuu = UBound(gDataAry())

    With sprList
